#include <bits/stdc++.h>
#include <vector>
#include <cmath>
#include <string.h>
using namespace std;
#define re return 0;
#define pb push_back
#define intt long long int
#define ll long long
#define mem memset
#define ca cout<<1<<endl
#define cb cout<<0<<endl
#define lab long double

void trying()
{
    int j,k;
    cin>>j>>k;
    
    if(j/k>=30){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
}
int main () {
    long long n;
    cin>>n;
    while(n--){
        trying();
    }
    
    return 0;
    
    
}
